import java.util.*;
class ContStmt3
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter number");
		var n=sc.nextInt();
		if(n%10==0) 
		{
		System.out.println(n + "has last digit of 0");
		}
		else
		{
			System.out.println(n+ "does not have last digit");
		}
	   
	}
}
